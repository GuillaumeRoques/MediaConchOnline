{% extends 'AppBundle:Default:base.html.twig' %}

{% block title %}
MediaConch Demo full file checker
{% endblock %}

{% block body %}
<h1>MediaConch Demo full file checker</h1>

<div>
{% if form %}
    {{ form(form) }}
{% else %}
    Quota exceeded, please contact us to raise your quotas.
{% endif %}
</div>

{% if checks %}
    <br />
    {{ include('AppBundle:Default:fileDetail.html.twig', { 'checks': checks }) }}
{% endif %}

{% endblock %}
